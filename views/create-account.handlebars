 <div class="col s5 login-section right">
        <form class="tutor-form">
            <div class="row create-section">
                <div class="col s12">
                    <h3>Create an Account</h3>
                </div>
            </div>
            <div class="row">
                <div class="col s6">
                    <input id="user-firstname" class="white-text" placeholder="First Name"></input>
                </div>
                <div class="col s6">
                    <input id="user-lastname" class="white-text" placeholder="Last Name"></input>
                </div>
            </div>
            <div class="row">
                <div class="col s12">
                    <input id="user-email" class="white-text" placeholder="Email"></input>
                </div>
            </div>
            <div class="row">
                <div class="col s12">
                    <input id="user-password" class="white-text" placeholder="Password" type="password"></input>
                </div>
            </div>
            <div class="row">
                <div class="col s12">
                    <input id="user-verify" class="white-text" placeholder="Verify Password" type="password"></input>
                </div>
            </div>
            <div class="row">
                <div class="col s12">
                    <button type="submit" class="btn">Create Account as User</button>
                </div>
            </div>
        </form>
    </div>
</div>
  <div class="row center">
            <img class="create-section" src="/images/Sign-up.png" alt="TECH-BLOG">
        </div>
    </div>
</div>
<script src="/javascript/create-account.js"></script>